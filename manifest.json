{
  "manifest_version": 3,
  "name": "Rooster Financial",
  "icons": {
    "16": "images/Scrooge Boc Boc 16.png",
    "32": "images/Scrooge Boc Boc 32.png",
    "48": "images/Scrooge Boc Boc 48.png",
    "128": "images/Scrooge Boc Boc 128.png"
  },
  "version": "1.0.0",
  "permissions": [ "storage" ],
  "action": { "default_popup": "index.html" },
  "background": {
    "service_worker": "src/scripts/background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["src/simplii/content.ts"],
      "matches": [
        "https://online.simplii.com/*"
      ]
    },
    {
      "js": ["src/questrade/content.ts"],
      "matches": [
        "https://my.questrade.com/clients/en/my_home"
      ]
    },
    {
      "js": ["src/tangerine/content.ts"],
      "matches": ["https://www.tangerine.ca/app/*"]
    },
    {
      "js" : ["src/wealthsimple/content.ts"],
      "matches": ["https://my.wealthsimple.com/app/home"]
    }
  ]
}